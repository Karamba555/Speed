config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/18000000.wifi'
	option band '2g'
	option txpower '100'
	option country 'US'
	option disabled '0'
	option legacy_rates '1'
	option hwmode '11a'
	option htmode 'HT20'
	option channel '11'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option ifname 'ra0'
	option ssid 'ALR-LTE-956347'
	option wds '1'
	option isolate '1'
	option port '1812'
	option auth_secret 'SecretForAP1'
	option key 'P9LoQqUka1234'
	option server '192.168.1.200'
	option encryption 'sae'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/18000000.wifi+1'
	option band '5g'
	option txpower '100'
	option country 'US'
	option disabled '0'
	option legacy_rates '0'
	option hwmode '11a'
	option htmode 'HE160'
	option channel '36'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option network 'lan'
	option ifname 'rai0'
	option ssid 'ALR-LTE_5G_956347'
	option wds '1'
	option isolate '1'
	option server '192.168.1.200'
	option port '1812'
	option auth_secret 'SecretForAP1'
	option key 'cWb7cqT31234'
	option encryption 'sae'